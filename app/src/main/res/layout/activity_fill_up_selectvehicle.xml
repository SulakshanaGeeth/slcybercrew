<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Fill_Up_Selectvehicle"
    android:background="#d7e0da">

    <include
        android:id="@+id/include4"
        layout="@layout/toolbar2"></include>

    <Spinner
        android:id="@+id/spn_fill_up_select_vehicle"
        android:layout_width="190dp"
        android:layout_height="66dp"
        android:layout_marginTop="68dp"
        android:background="@android:drawable/btn_dropdown"
        android:spinnerMode="dropdown"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_fillUp_select_vehicle"
        tools:ignore="SpeakableTextPresentCheck" />

    <Button
        android:id="@+id/btn_fill_up_select"
        android:layout_width="115dp"
        android:layout_height="57dp"
        android:layout_marginBottom="208dp"
        android:text="@string/btn_fill_up_select"
        android:textSize="22sp"
        app:backgroundTint="@color/colorPrimary"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/tv_fillUp_select_vehicle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="172dp"
        android:text="@string/tv_fillUp_select_vehicle"
        android:textColor="@color/black"
        android:textSize="24sp"
        android:textStyle="bold"
        android:typeface="monospace"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/include4" />
</androidx.constraintlayout.widget.ConstraintLayout>